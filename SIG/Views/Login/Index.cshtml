@{
    ViewBag.Title = "SIG-Login";
    Layout = "~/Views/Shared/_MasterPageLogin.cshtml";
}

<form role="form" id="login_autentication">
    <fieldset>
        <div class="form-group">
            <input class="form-control" placeholder="E-mail" id="edtEmail" name="email" type="email" autofocus>
        </div>
        <div class="form-group">
            <input class="form-control" placeholder="Senha" id="edtSenha" name="password" type="password" value="">
        </div>
        <div class="checkbox">
            <label>
                <input name="remember" type="checkbox" value="Remember Me">Lembrar Acesso
            </label>
        </div>
        <!-- Change this to a button or input when using this as a form -->
        <button href="index.html" class="btn btn-lg btn-success btn-block">Acessar</button>
    </fieldset>
</form>

<script>
    function teste() {
        //console.log("submit login");

        setTimeout(function () {
            document.querySelector('#edtSenha').value = '';

        }, 1000);
    }

</script>